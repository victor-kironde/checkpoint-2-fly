$('#results').empty()
<% errors = check_search(params[:origin], params[:destination], params[:date]) %>
<%if errors.any? %>
  $('#search-errors')
    .html("<%= j(render partial: 'partials/search_errors', locals: { errors: errors } ) %>");
  <% else %>
  $('#results')
    .html("<%= j(render partial: 'partials/flight_results', locals: { flights: @flights}) %>");
<% end %>
